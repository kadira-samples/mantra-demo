    "babel-polyfill": "6.x.x",
    "babel-preset-es2015": "6.x.x",
    "babel-preset-react": "6.x.x",
    "babel-preset-stage-2": "6.x.x",
    "babel-root-slash-import": "1.x.x",
    "chai": "3.x.x",
    "enzyme": "^2.2.0",
    "eslint": "2.8.x",
    "eslint-plugin-react": "5.0.x",
    "jsdom": "^8.4.0",
    "mocha": "2.x.x",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.0.1",
    "sinon": "1.17.x",
    "style-loader": "^0.13.1"
  },
  "dependencies": {
    "domready": "^1.0.8",
